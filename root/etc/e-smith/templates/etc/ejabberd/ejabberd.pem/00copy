{
    my $file = '/etc/pki/tls/certs/slapd.pem';
    my %lines = ();
    my @lines = ();
    open (RD, "<$file")
    || warn "Cannot open input file $file: $!\n";
    while (<RD>)
    {
        chop;
        push @lines, $_;
        $lines{$_} = 1;
    }
    close(RD);
    $OUT .= "";
    foreach my $line (@lines)
    {
        $OUT .= "$line\n";
    }

}
